<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Papyrus.Desktop.Features.Topics" x:Class="Papyrus.Desktop.Features.Topics.TopicWindow"
        mc:Ignorable="d"
        Title="TopicWindow"
        DataContext="{Binding ViewModel, RelativeSource={RelativeSource Self}}">
    <Window.Resources>
        <local:VersionRangesToVmConverter x:Key="VersionRangesToVmConverter" />
    </Window.Resources>
    <Grid>
        <local:VersionRangesUserControl HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top" Height="NaN" Width="NaN" Grid.RowSpan="2" DataContext="{Binding EditableTopic.VersionRanges,Converter={StaticResource VersionRangesToVmConverter}}"/>
    </Grid>
</Window>
