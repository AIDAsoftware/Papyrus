{
  "env": {
    "node": true,
    "browser" : true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures" : {
        "jsx": true
    }
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "no-var": 1,
    "no-console": 0,
    "semi": 2,
    "quotes": [2, "single"]
  },
  "plugins" : [
      "react"
  ]
}